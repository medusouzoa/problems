
import java.util.Scanner;
public class problem3 {
    public static void main(String[] args) {
   
   /*You will type an integer between 0 and 100. This program will add all digits in the integer. 
     For example : If  you type 95,  program will give you 14.
 */
  
    Scanner input = new Scanner (System.in);
        System.out.println("Enter your integer= ");
   double yours = input.nextDouble();
           double result = (yours %10) + (yours /10);
           System.out.println("The result is " + result);
            }
    
}
